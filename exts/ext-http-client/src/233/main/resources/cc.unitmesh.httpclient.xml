<idea-plugin package="cc.unitmesh.httpclient">
    <!--suppress PluginXmlValidity -->
    <dependencies>
        <plugin id="com.jetbrains.restClient"/>
    </dependencies>

    <extensions defaultExtensionNs="cc.unitmesh">
        <httpClientExecutor implementation="cc.unitmesh.httpclient.IntellijHttpClientExecutor"/>

        <runService implementation="cc.unitmesh.httpclient.HttpClientFileRunService"/>
        <shireHttpHandler implementation="cc.unitmesh.httpclient.CUrlHttpHandler"/>
    </extensions>
</idea-plugin>
