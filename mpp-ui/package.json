{
  "name": "@autodev/cli",
  "version": "0.1.4",
  "description": "AutoDev CLI - Terminal UI for AI-powered development assistant",
  "type": "module",
  "bin": {
    "autodev": "./dist/jsMain/typescript/index.js"
  },
  "scripts": {
    "build:kotlin": "cd .. && ./gradlew :mpp-core:assembleJsPackage",
    "build:ts": "tsc && chmod +x dist/jsMain/typescript/index.js",
    "build": "npm run build:kotlin && npm run build:ts",
    "dev": "tsc --watch",
    "start": "node dist/jsMain/typescript/index.js",
    "test": "vitest run",
    "test:unit": "vitest run src/jsMain/typescript/**/*.test.ts",
    "test:integration": "vitest run src/test/integration/**/*.test.ts",
    "test:integration:simple": "vitest run src/test/integration/simple-robustness.test.ts",
    "test:integration:video": "vitest run src/test/integration/business-video-support.test.ts",
    "test:integration:jwt": "vitest run src/test/integration/business-jwt-auth.test.ts",
    "test:integration:upgrade": "vitest run src/test/integration/business-spring-upgrade.test.ts",
    "test:integration:graphql": "vitest run src/test/integration/business-graphql-api.test.ts",
    "test:robustness": "npm run test:integration:simple && npm run test:integration:video && npm run test:integration:jwt",
    "test:business": "npm run test:integration:video && npm run test:integration:jwt && npm run test:integration:upgrade && npm run test:integration:graphql",
    "test:completion": "node test-completion.js",
    "test:cli": "node test-cli-completion.js",
    "test:all": "npm test && npm run test:completion && npm run test:cli",
    "test:ci": "npm run build && npm run test:integration",
    "test:framework": "node src/test/framework/validate-structure.cjs",
    "test:integration-v2": "npm run build:ts && npm test src/test/integration-v2 -- --reporter=verbose",
    "test:integration-v2:simple": "npm run build:ts && npm test src/test/integration-v2/simple-robustness.test.ts -- --reporter=verbose",
    "test:integration-v2:business": "npm run build:ts && npm test src/test/integration-v2/business-scenarios.test.ts -- --reporter=verbose",
    "test:integration-v2:errors": "npm run build:ts && npm test src/test/integration-v2/error-recovery.test.ts -- --reporter=verbose",
    "test:integration-v2:performance": "npm run build:ts && npm test src/test/integration-v2/performance.test.ts -- --reporter=verbose",
    "test:integration-v2:custom": "npm run build:ts && npm test src/test/integration-v2/custom-scenarios.test.ts -- --reporter=verbose",
    "analyze-test-results": "node scripts/analyze-test-results.cjs",
    "clean": "rm -rf dist build",
    "prepublish:local": "npm run build",
    "publish:local": "node scripts/publish-local.js",
    "prepublish:remote": "npm run build",
    "publish:remote": "node scripts/publish-remote.js",
    "postinstall": "node scripts/check-mpp-core.js || echo 'Warning: mpp-core not found, run npm run build:kotlin first'"
  },
  "packageManager": "pnpm@10.20.0",
  "keywords": [
    "ai",
    "cli",
    "terminal",
    "coding-assistant",
    "autodev",
    "tui"
  ],
  "author": "AutoDev Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@autodev/mpp-core": "^0.1.4",
    "@js-joda/core": "^5.6.5",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "diff": "^7.0.0",
    "dotenv": "^16.4.5",
    "highlight.js": "^11.11.1",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/diff": "^6.0.0",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.12",
    "ink-testing-library": "^4.0.0",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist/",
    "README.md"
  ]
}
